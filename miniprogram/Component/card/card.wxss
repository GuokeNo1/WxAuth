@import "/Style/steam.wxss";
@import "/Style/demo.wxss";

.card{
  margin-left: 25rpx;
  margin-right: 25rpx;
  margin-top: 10rpx;
  border-radius: 25rpx;
  width: 700rpx;
  height: 200rpx;
  overflow: hidden;
  display: flex;
  background-color: #fff;
}

.card .icon{
  width: 150rpx;
  height: 150rpx;
  margin: 25rpx;
  background-size: 100%;
}
.info{
  flex: auto;
  padding:10rpx;
  display: flex;
  justify-content: space-between;
}
.text{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.name{
  font-size: large;
}
.token{
  font-size: xx-large;
}
.state_icon{
  width: 40rpx;
  height: 40rpx;
  margin: 10rpx;
  background-size: contain;
  /* background-image: url("data:image/png;base64,"); */
}
.state_cloud{
  background-image: url("data:image/png;base64,");
}